{
  "version": 2,
  "buildCommand": "cd angularapp && npm install && npm run build && cp dist/angularapp/browser/index.csr.html dist/angularapp/browser/index.html",
  "outputDirectory": "angularapp/dist/angularapp/browser",
  "installCommand": "cd angularapp && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|woff|woff2|ttf|eot|svg|png|jpg|jpeg|gif|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

